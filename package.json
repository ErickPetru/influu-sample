{
  "name": "influu-sample",
  "description": "Exemplo de repositório para o projeto Influu MidiaKit construído com Vue.js, Nuxt.js e Tailwind CSS",
  "version": "1.0.0",
  "author": "Erick Eduardo Petrucelli <erick.petrucelli@fatectq.edu.br>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development HOST=localhost PORT=3000 nuxt",
    "start": "cross-env NODE_ENV=production HOST=0.0.0.0 PORT=80 nuxt start",
    "build": "nuxt build --modern=client",
    "generate": "nuxt generate --modern",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "analyze": "nuxt build --modern=client --analyze"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "nuxt": "^2.12.2"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/svg": "^0.1.11",
    "@nuxtjs/tailwindcss": "^1.4.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.1.3",
    "nuxt-webfontloader": "^1.1.0",
    "postcss-color-mod-function": "^3.0.3",
    "prettier": "^1.19.1",
    "vue-resize-directive": "^1.2.0"
  }
}
